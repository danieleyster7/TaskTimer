<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" styleClass="background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="deyster.timer.dialog.DeleteTaskController">
   <children>
      <TableView fx:id="taskTable" prefHeight="200.0" prefWidth="200.0" styleClass="background" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="125.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="taskNameColumn" maxWidth="175.0" prefWidth="175.0" text="Task" />
        </columns>
      </TableView>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="175.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleOK" prefHeight="30.0" prefWidth="100.0" text="Delete">
               <VBox.margin>
                  <Insets left="12.5" top="60.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleCancel" prefHeight="30.0" prefWidth="100.0" text="Close">
               <VBox.margin>
                  <Insets bottom="50.0" left="12.5" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../view/JMetroDarkTheme.css" />
      <URL value="@../view/DarkTheme.css" />
   </stylesheets>
</AnchorPane>
